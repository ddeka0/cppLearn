    $(document).ready(function() {
  $('#p-refresh').on('click', function() {
    var $this = $(this);
    var loadingText = '<div class="preview-spin"></div>&nbsp; Refreshing previews...';
    if ($(this).html() !== loadingText) {
      $this.data('original-text', $(this).html());
      $this.html(loadingText);
    }
    setTimeout(function() {
      $this.html($this.data('original-text'));
    }, 1400);
  });
})
  
function reloadimg() {
  var rnd_arr = [];

  for (let i = 1; i <= 6;) {
    rnd = '';

    rnd = Math.floor(Math.random() * (16 + 1 - 1) + 1);
    if (rnd_arr.indexOf(rnd) < 0) {

      rnd_arr.push(rnd);
      i++;
    }
  }
  rnd_arr.sort(function (a, b) {
    return a - b
  });

  y = 0;
  while (y <= 5) {
    document.getElementById('img' + y).src = "jpg" + rnd_arr[y] + ".jpg";
    y++;
  }
}

</script>

